## exports issue links - for "Related Issues" in pdf
#macro(issueLinks $linkName $linkedIssues)
	#if($linkedIssues && !$linkedIssues.empty)
		#foreach ($linkedIssue in $linkedIssues)
        	[#if($linkedIssue.resolutionObject)<fo:inline text-decoration="line-through">#end$xmlutils.escape($linkedIssue.key)#if($linkedIssue.resolutionObject)</fo:inline>#end]
            	- $xmlutils.escape($linkedIssue.summary)
		#end
	#end
#end


